# Server Configuration
PORT=10000
NODE_ENV=development

# Storage Configuration
STORAGE_DIR=/data

# Mirror Node Configuration
MIRROR_NODE_URL=https://mainnet-public.mirrornode.hedera.com/api/v1
MIRROR_NODE_TIMEOUT=30000
MAX_RETRIES=3

# CORS Configuration
ALLOWED_ORIGINS=https://hedera-token-analyzer.netlify.app,http://localhost:5173

# Rate Limiting
RATE_LIMIT=150
BATCH_SIZE=50
HOLDER_BATCH_SIZE=25
PROCESSING_DELAY=200
